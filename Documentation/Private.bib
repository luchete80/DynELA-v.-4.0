	file = {Nistor - 2005 - Identification expérimentale et simulation numériq.pdf:/home/pantale/SynologyDrive/Recherche/Bibliography/Zotero/storage/Z5ZIKFH7/Nistor - 2005 - Identification expérimentale et simulation numériq.pdf:application/pdf}
}

@phdthesis{goidescu_caracterisation_2011,
	type = {{PhD} {Thesis}},
	title = {Caractérisation et modélisation de l'endommagement par microfissuration des composites stratifiés - {Apports} des mesures de champs et de l'homogénéisation},
	url = {http://www.theses.fr/2011INPT0065},
	abstract = {Ce travail porte sur l'endommagement des matériaux composites stratifiés utilisés notamment pour la réalisation de pièces structurales minces. La dégradation de ces matériaux induite par la création et le développement de surfaces de décohésion internes est abordée sous deux angles. Une campagne expérimentale a tout d'abord été menée sur des stratifiés en carbone-époxy réalisés par infusion de résine liquide et sollicités en traction uniaxiale. Cette étude propose une analyse originale à l'aide de trois techniques optiques permettant une caractérisation de l'endommagement par mesures de champs : cinématiques (par stéréo-corrélation d'images), thermiques (par thermographie infrarouge) et densimétriques (par tomographie à rayons X). Le second volet du travail concerne la modélisation de la microfissuration dans le contexte d'une anisotropie initiale. A cette fin, une homogénéisation bidimensionnelle de milieux orthotropes fissurés permet la prise en compte de défauts d'orientation arbitraire et des effets unilatéraux (ouverture-fermeture des microfissures) au sein d'une formulation énergétique en déformation. Sur cette base, un modèle de comportement est proposé dans le cadre de la thermodynamique des processus irréversibles avec variables internes. Des simulations numériques permettent de démontrer les capacités prédictives de la formulation, en particulier la représentation du comportement non linéaire de ces matériaux, l'interaction entre les anisotropies initiale et induite et la restitution des propriétés élastiques lors de la fermeture de défauts.},
	urldate = {2017-07-07},
	school = {Toulouse, INPT},
	author = {Goidescu, Cristina},
	month = sep,
	year = {2011},
	file = {Goidescu - 2011 - Caractérisation et modélisation de l'endommagement.pdf:/home/pantale/SynologyDrive/Recherche/Bibliography/Zotero/storage/JE3GIGWN/Goidescu - 2011 - Caractérisation et modélisation de l'endommagement.pdf:application/pdf}
}

@phdthesis{abbassi_federation_2008,
	type = {{PhD} {Thesis}},
	title = {Fédération des connaissances de mise en forme dans une plateforme de prototypage virtuel},
	url = {http://www.theses.fr/2008INPT051H},
	abstract = {Une large campagne expérimentale a été engagée au cours de ce travail. Dans un premier temps on s'intéresse à l'identification classique à partir d'essais de traction. Une série d'essais de gonflement hydraulique à matrice variable a également été développée. Des techniques de mesure sans contact ont été utilisées au cours des essais d'identification et pour l'analyse de la qualité du produit fini. Les essais de gonflement hydrauliques ont été exploités dans un protocole d'identification paramétrique basé sur un couplage numérique/expérimental. Des algorithmes de type Monte-Carlo et Levenberg-Marquardt ont été utilisés. Des observations sur MEB des zones de rupture sur les éprouvettes d'essai ont été effectuées. Ceci nous a permi de choisir un modèle d'endommagement ductile de type GTN pour la simulation numérique. Ce modèle a été utilisé pour la prédiction de la rupture sur des pièces obtenues par déformation plastique. La validation expérimentale des prédictions a été effectuée.},
	urldate = {2020-03-24},
	school = {Toulouse, INPT},
